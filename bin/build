#!/usr/bin/env bash

# Delete existing files
rm -rf docs
rm -rf lib/wealthsimple/api
rm -rf lib/wealthsimple/models
rm -rf spec/api
rm -rf spec/models

swagger-codegen generate \
 --input-spec https://api.sandbox.wealthsimple.com/v1/swagger.json \
 --output . \
 --lang ruby \
 --template-dir swagger/templates \
 --ignore-file-override swagger/.swagger-codegen-ignore  \
 --config swagger/config.json
